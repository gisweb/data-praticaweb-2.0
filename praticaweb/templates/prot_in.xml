<?xml version="1.0" encoding="utf-8"?>
<Segnatura
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:tal="http://xml.zope.org/namespaces/tal"
        xmlns:metal="http://xml.zope.org/namespaces/metal"
        xsi:noNamespaceSchemaLocation=".\DOCAREAProto.xsd"
        tal:define="mittente view/document">
    <Intestazione>
        <Oggetto tal:content="python:context.getItem('descrizione_intervento','manca la descrizione intervento')"></Oggetto>
        <Identificatore>
            <CodiceAmministrazione  tal:content="view/destinatario/CodiceAmministrazione">1</CodiceAmministrazione>
            <CodiceAOO tal:content="view/destinatario/CodiceA00">1</CodiceAOO>
            <NumeroRegistrazione>0</NumeroRegistrazione>
            <DataRegistrazione>0</DataRegistrazione>
            <Flusso>E</Flusso>
        </Identificatore>
        <Mittente>
            <Persona tal:attributes="id view/mittente/CodiceFiscale">
                <Nome tal:content="view/mittente/Nome">Nome</Nome>
                <Cognome tal:content="view/mittente/Cognome">Cognome</Cognome>
                <CodiceFiscale tal:content="view/mittente/CodiceFiscale">Codice fiscale</CodiceFiscale>
                <IndirizzoTelematico tal:content="view/mittente/IndirizzoTelematico">email</IndirizzoTelematico>
            </Persona>
        </Mittente>
        <Destinatario>
            <Amministrazione>
                <Denominazione tal:content="view/destinatario/Denominazione">Denominazione</Denominazione>
                <CodiceAmministrazione tal:content="view/destinatario/CodiceAmministrazione">1010</CodiceAmministrazione>
                <IndirizzoTelematico tal:content="view/destinatario/IndirizzoTelematico"></IndirizzoTelematico>
                <UnitaOrganizzativa/>
            </Amministrazione>
            <AOO>
                <CodiceAOO tal:content="view/destinatario/CodiceA00">codice</CodiceAOO>
            </AOO>
            <IndirizzoTelematico />
        </Destinatario>
        <Classifica>
            <CodiceAmministrazione tal:content="view/destinatario/CodiceAmministrazione">1010</CodiceAmministrazione>
            <CodiceAOO tal:content="view/destinatario/CodiceA00">1</CodiceAOO>
            <CodiceTitolario tal:content="view/destinatario/CodiceTitolario"></CodiceTitolario>
        </Classifica>
    </Intestazione>
    <Descrizione>
        <Documento tal:attributes="nome view/fileName;id view/objId">
            <DescrizioneDocumento tal:content="context/Title">Titolo</DescrizioneDocumento>
            <TipoDocumento tal:content="python:'PRATICAWEBSUAP' if context.getItem('iol_tipo_proc')=='suap' else 'PRATICAWEBSUE'">TIPO RICHIESTA</TipoDocumento>
        </Documento>
        <Allegati/>
    </Descrizione>
    <ApplicativoProtocollo nome="AGSPR">
        <Parametro nome="tipoSmistamento" valore="COMPETENZA"/>
    </ApplicativoProtocollo>
</Segnatura>
